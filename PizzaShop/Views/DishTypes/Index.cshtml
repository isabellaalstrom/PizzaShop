@model IEnumerable<DishType>

@{
    ViewData["Title"] = "Index";
}

<h2>Dish Types</h2>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DishTypeName)
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DishTypeName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DishTypeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DishTypeId">Details</a> |
                <a id="dishType-@item.DishTypeId" asp-action="Delete" asp-route-id="@item.DishTypeId">Delete</a>
            </td>
        </tr>
    }

    </tbody>
</table>
<p class="text-danger">@ViewBag.Error</p>