@model IEnumerable<Dish>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>
<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DishName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th>@Html.DisplayNameFor(model => model.DishIngredients)</th>
        <th>@Html.DisplayNameFor(model => model.DishType)</th>
        <th></th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model) {
        {
            await Html.RenderPartialAsync("_DishPartial", item);
        }
    }
    </tbody>
</table>
